<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.pe.recife.tafeito">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".gui.FornecedorLoginActivity"
            android:theme="@style/AppTheme.Dark">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".gui.FornecedorRegistroActivity"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name=".gui.FornecedorPrincipalActivity"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name=".gui.FornecedorAgendamentoActivity"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name=".gui.FornecedorOfertaActivity"
            android:theme="@style/AppTheme.Dark" />
        <activity android:name=".gui.FornecedorServicoActivity" />
        <activity android:name=".gui.TesteBDActivity" />
        <activity
            android:name=".gui.FornecedorLoginRESTActivity"
            android:theme="@style/AppTheme.Dark">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".gui.FornecedorRegistroRESTActivity"
            android:theme="@style/AppTheme.Dark">
        </activity>
    </application>

</manifest>